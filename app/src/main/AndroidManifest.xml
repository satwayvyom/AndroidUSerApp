<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.vyom.gpstrackersatway">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <supports-screens
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/splash"
        android:usesCleartextTraffic="true"
        android:requestLegacyExternalStorage="true">
        <activity
            android:name=".Notification_view"
            android:exported="true" />
        <activity
            android:name=".Notification_Demo"
            android:exported="true" />
        <activity android:name=".list_item" />
        <activity android:name=".History_Date_Recyclerview" />
        <activity android:name=".reset_pass" />
        <activity android:name=".Reset_password" />
        <activity android:name=".HistoryStop_map" />
        <activity android:name=".Stopped_recyclerview" />
        <activity android:name=".stopped_history" />
        <activity android:name=".Map_History" />
        <activity android:name=".activity_main1" />
        <activity android:name=".Table_Details" />
        <activity android:name=".RecyclerView_MoreDetails" />
        <activity android:name=".mapview2" />
        <activity
            android:name=".Vehicle_Details"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN"
                    android:screenOrientation="portrait" />

                <category
                    android:name="android.intent.category.LAUNCHER"
                    android:screenOrientation="portrait" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LocationTracking"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />

        <activity
            android:name=".CustomView1"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MapView"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:screenOrientation="portrait"
                android:value="com.vyom.gpstrackersatway.recyclerview" />
        </activity>
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main2"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".recyclerview"
            android:screenOrientation="portrait" />
        <activity
            android:name=".service"
            android:screenOrientation="portrait" />
        <activity
            android:name=".alerts"
            android:screenOrientation="portrait" />
        <activity
            android:name=".settings"
            android:screenOrientation="portrait" />
        <activity
            android:name=".myVehicleDatalist"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Movelist"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contact_us"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Splash"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Home"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>