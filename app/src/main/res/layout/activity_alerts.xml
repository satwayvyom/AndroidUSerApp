<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/loginwall"
    android:foreground="@color/cardview_shadow_end_color"
    tools:context=".alerts">


    <androidx.cardview.widget.CardView
        android:layout_width="412dp"
        android:layout_height="448dp"
        android:layout_marginBottom="4dp"
        android:background="@drawable/edittextborder"
        app:cardBackgroundColor="@color/loginbox"
        app:layout_constraintBottom_toTopOf="@+id/guideline7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/guideline39"
        app:layout_constraintVertical_bias="0.333"
        tools:ignore="MissingConstraints">

        <AutoCompleteTextView
            android:id="@+id/vehicleno"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/formtextbox"
            android:gravity="center"
            android:hint="Vehicle number"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textColorHint="@color/graylight" />

        <Button
            android:id="@+id/btnPick"
            android:layout_width="51dp"
            android:layout_height="51dp"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="300dp"
            android:layout_marginTop="120dp"
            android:layout_marginBottom="4dp"
            android:background="@drawable/calendd"
            android:scaleX="0.6"
            android:scaleY="0.6" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_centerInParent="true"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="120dp"
            android:background="@drawable/formtextbox"
            android:gravity="center"
            android:hint="Date"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textColorHint="@color/graylight"
            android:textSize="16sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btnPick2"
            android:layout_width="51dp"
            android:layout_height="51dp"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="300dp"
            android:layout_marginTop="180dp"
            android:layout_marginBottom="4dp"
            android:background="@drawable/calendd"
            android:scaleX="0.6"
            android:scaleY="0.6" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="200dp"
            android:layout_height="51dp"
            android:layout_centerInParent="true"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="180dp"
            android:background="@drawable/formtextbox"
            android:gravity="center"
            android:hint="Date"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textColorHint="@color/graylight"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:ignore="DuplicateIds" />

        <Button
            android:id="@+id/btnrun"
            android:layout_width="100dp"
            android:layout_height="51dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="240dp"
            android:width="150dp"
            android:height="20dp"
            android:background="@color/loginbox"
            android:text="STATUS :"
            android:textAlignment="textEnd"
            android:textColor="@color/white"
            android:textSize="15dp"
            android:textStyle="bold"
            tools:ignore="NotSibling,UnknownId" />

        <Button
            android:id="@+id/button12"
            android:layout_width="207dp"
            android:layout_height="46dp"
            android:layout_marginLeft="103dp"
            android:layout_marginTop="330dp"
            android:background="@drawable/loginbuttonstyle"
            android:text="Details"
            android:textStyle="bold" />

        <Spinner
            android:id="@+id/spinner"
            android:layout_width="200dp"
            android:layout_height="51dp"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="240dp"
            android:background="@drawable/spinner_design"
            android:foregroundTint="@color/loginwall"
            android:spinnerMode="dropdown"
            android:dropDownSelector="@color/black"
            android:textAlignment="center"

            />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="100dp"
            android:layout_height="51dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="180dp"
            android:gravity="center"
            android:text="TO DATE:'"
            android:textAlignment="textEnd"
            android:textColor="@color/white"
            android:textSize="15dp"
            android:textStyle="bold"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="100dp"
            android:layout_height="51dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="120dp"
            android:gravity="center"
            android:text="FROM DATE:"
            android:textAlignment="textEnd"
            android:textColor="@color/white"
            android:textSize="15dp"
            android:textStyle="bold"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="100dp"
            android:layout_height="51dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="60dp"
            android:gravity="center"
            android:text="VEHICLE NO :"
            android:textAlignment="textEnd"
            android:textColor="#FFFFFF"
            android:textSize="15dp"
            android:textStyle="bold"
            tools:ignore="RtlCompat" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="215dp"
        android:layout_height="46dp"
        android:layout_gravity="center_horizontal"
        android:gravity="center"
        android:text="History"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20dp"

        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline6"
        app:layout_constraintEnd_toStartOf="@+id/guideline34"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline33"
        app:layout_constraintTop_toTopOf="@+id/guideline4"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="1.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.06" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.12" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="575dp" />

    <!--    <androidx.constraintlayout.widget.Guideline-->
    <!--        android:id="@+id/guideline9"-->
    <!--        android:layout_width="wrap_content"-->
    <!--        android:layout_height="wrap_content"-->
    <!--        android:orientation="horizontal"-->
    <!--        app:layout_constraintGuide_percent="" />-->

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.24" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.76" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.17236662" />


</androidx.constraintlayout.widget.ConstraintLayout>