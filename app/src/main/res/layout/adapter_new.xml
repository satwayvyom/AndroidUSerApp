<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="175dp"
    android:background="@color/loginwall">

    <androidx.cardview.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="match_parent"
        android:layout_height="152dp"
        android:animateLayoutChanges="true"
        android:animationCache="false"
        android:background="@drawable/edittextborder"
        android:padding="5dp"
        app:cardBackgroundColor="@color/loginbox"
        app:cardCornerRadius="15dp"
        card_view:elevation="5dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="45dp">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="160dp"
            android:padding="20dp">

            <TextView
                android:id="@+id/textTitle"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="120dp"
                android:gravity="center"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="16dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.443"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/modetxt4"
                android:layout_width="0dp"
                android:layout_height="47dp"
                android:layout_marginStart="72dp"
                android:gravity="center"
                android:hint=""
                android:shadowColor="#000"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textColorHint="@color/Gray"
                android:textSize="11sp"
                android:visibility="visible"
                app:layout_constraintStart_toStartOf="parent"
                card_view:layout_editor_absoluteY="25dp"
                tools:ignore="RtlCompat" />

            <ImageButton
                android:id="@+id/sharebtn"
                android:layout_width="46dp"
                android:layout_height="46dp"
                android:background="@drawable/cardbuttonstyle"
                android:padding="8dp"
                android:scaleType="centerInside"
                android:tint="@color/yellow1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageButton2"
                app:layout_constraintHorizontal_bias="0.919"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/modetxt"
                app:layout_constraintVertical_bias="0.661"
                app:srcCompat="@drawable/whtssss" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="46dp"
                android:layout_height="46dp"
                android:adjustViewBounds="true"
                android:background="@drawable/cardbuttonstyle"
                android:cropToPadding="false"
                android:padding="8dp"
                android:scaleType="centerInside"
                android:src="@drawable/cardicon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.915"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/modetxt"
                app:layout_constraintVertical_bias="0.861" />

            <TextView
                android:id="@+id/editTextTextPersonName"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="4dp"

                android:ems="10"
                android:gravity="center"
                android:text="IMEI            :"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textSize="12dp"
                app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.95"
                tools:ignore="RtlCompat" />

            <TextView
                android:id="@+id/modetxt"
                android:layout_width="110dp"
                android:layout_height="29dp"
                android:gravity="center"
                android:shadowColor="#000"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textColorHint="@color/Gray"
                android:textSize="12sp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_weight="0.3"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:ignore="RtlCompat" />

            <TextView
                android:id="@+id/editTextTextPersonName2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:ems="10"
                android:gravity="center"
                android:text="efere"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/modetxt"
                app:layout_constraintHorizontal_bias="0.893"
                app:layout_constraintStart_toEndOf="@+id/imeitxt"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/imeitxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:gravity="center"
                android:shadowColor="#000"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textColorHint="@color/white"
                android:textSize="12sp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.21"
                app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.00999999"
                tools:ignore="RtlCompat" />

            <TextView
                android:id="@+id/modetxt2"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:shadowColor="#000"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textColorHint="@color/Gray"
                android:textSize="12sp"
                android:visibility="visible"
                card_view:layout_editor_absoluteX="70dp"
                card_view:layout_editor_absoluteY="92dp"
                tools:ignore="RtlCompat" />

            <TextView
                android:id="@+id/modetxt3"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:hint=""
                android:shadowColor="#000"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textColorHint="@color/Gray"
                android:textSize="12sp"
                android:visibility="visible"
                app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName4"
                card_view:layout_editor_absoluteY="53dp"
                tools:ignore="RtlCompat" />

            <TextView
                android:id="@+id/editTextTextPersonName4"
                android:layout_width="78dp"
                android:layout_height="30dp"
                android:ems="10"
                android:gravity="center"
                android:text="Speed         :"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textSize="12dp"
                app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName5"
                app:layout_constraintVertical_bias="0.0"
                tools:ignore="RtlCompat" />

            <TextView
                android:id="@+id/editTextTextPersonName5"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:ems="10"
                android:gravity="center"
                android:text="Location     :"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textSize="12dp"
                app:layout_constraintStart_toStartOf="parent"
                card_view:layout_editor_absoluteY="25dp"
                tools:ignore="RtlCompat" />

            <TextView
                android:id="@+id/editTextTextPersonName3"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_marginTop="36dp"
                android:ems="10"
                android:gravity="center"
                android:text="Distance     :"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textSize="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName5"
                tools:ignore="RtlCompat" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>